set number
set relativenumber
set expandtab
set shiftwidth=2
set tabstop=2
set softtabstop=2
set autoindent
set smartindent
set nowrap
set cursorline
set clipboard=unnamedplus
syntax on
filetype plugin indent on

nnoremap <SPACE> <Nop>
let mapleader=" "
nnoremap <Leader>w :w<CR>
nnoremap <Leader>q :q<CR>

